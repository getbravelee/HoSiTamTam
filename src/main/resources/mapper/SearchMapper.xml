<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.suleekyuri.hositamtam.mapper.SearchMapper">
    <select id="findByLocationContaining" resultType="com.suleekyuri.hositamtam.search.dto.SuggestionDto">
        SELECT id, location_name as locationName FROM location WHERE location_name LIKE CONCAT('%', #{keyword}, '%')
    </select>
</mapper>
